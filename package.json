{
  "name": "infokala",
  "version": "0.0.0",
  "description": "Info log management system for Desucon and Tracon",
  "scripts": {
    "prepublish": "pip install -r requirements.txt && bower install && gulp build",
    "start": "gulp",
    "test": "python manage.py test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/japsu/infokala"
  },
  "author": "Santtu Pajukanta",
  "license": "MIT",
  "homepage": "https://github.com/japsu/infokala",
  "devDependencies": {
    "bluebird": "^2.3.6",
    "bower": "^1.3.12",
    "browserify": "^5.12.0",
    "browserify-shim": "^3.7.0",
    "coffee-script": "^1.8.0",
    "coffeeify": "^0.7.0",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^1.0.0",
    "gulp-jade": "^0.8.0",
    "gulp-minify-css": "^0.3.10",
    "gulp-rename": "^1.2.0",
    "gulp-streamify": "0.0.5",
    "gulp-stylus": "^1.3.3",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "mocha": "^1.21.4",
    "nib": "^1.0.3",
    "vinyl-source-stream": "^1.0.0"
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.min.js",
    "knockout": "./bower_components/knockout/dist/knockout.js",
    "knockout-mapping": "./bower_components/knockout-mapping/knockout.mapping.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.min.js"
  },
  "browserify-shim": {
    "jquery": "jQuery",
    "bootstrap": {
      "depends": "jquery:$",
      "exports": "jQuery"
